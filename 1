function assertIsNull(value) {
    if (value !== null) {
        throw new Error('Value is not null');
    }
}

function assertIsNotNull(value) {
    if (value === null) {
        throw new Error('Value is null');
    }
}

// Example usage
try {
    assertIsNull(value); // Replace 'value' with the variable to be checked
    console.log('Value is null');
} catch (error) {
    console.error(error.message);
}

try {
    assertIsNotNull(value); // Replace 'value' with the variable to be checked
    console.log('Value is not null');
} catch (error) {
    console.error(error.message);
}

